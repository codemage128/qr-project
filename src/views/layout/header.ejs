                </div>
                <!-- Title -->
                <h5 class="h5 mb-3">Upgrade to Pro</h5>
                <!-- Text -->
                <p class="text-muted">
                  One year support, montlhy updates for up to 5 team members.
                </p>
                <!-- Button -->
                <a href="#" class="btn btn-sm btn-warning">Upgrade</a>
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer py-3 mt-auto">
          <a href="/log-out" class="btn btn-block btn-sm btn-neutral btn-icon rounded-pill">
            <span class="btn-inner--icon"><i data-feather="log-out"></i></span>
            <span class="btn-inner--text">Sign out</span>
          </a>
        </div>
      </div>
    </div>
  </div>
  <%- include('cookie.ejs')%>
